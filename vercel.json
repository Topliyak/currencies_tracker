{
    "version": 2,
    "framework": "flask",
    "installCommand": "pip install poetry | poetry install",
    "builds": [
        {
            "src": "server/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/app.py"
        }
    ]
}
